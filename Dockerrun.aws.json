{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name" : "client",
      "image" : "mrwin95/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name" : "server",
      "image" : "mrwin95/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name" : "worker",
      "image" : "mrwin95/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name" : "nginx",
      "image" : "mrwin95/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [{
        "containerPort": 80,
        "hostPort": 80
      }],
      "links": ["client","server"]
    }
  ]
}